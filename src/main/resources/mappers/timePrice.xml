    <!--table7.원데이 상품등록-->
    <insert id="onedayPriceInsert"
            parameterType="com.Gosk.GoskProject20221221.domain.TimePrice"
            useGeneratedKeys="true"
            keyProperty="oneday_price_id"
    >
        insert into
        oneday_price_mst(
        oneday_price_id,
        oneday_time,
        oneday_price
        )
        values(
        0,
        #{oneday_time},
        #{oneday_price}
        )
    </insert>

