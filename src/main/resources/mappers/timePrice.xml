        values(
        0,
        #{commuter_dp_time},
        #{commuter_dp_price}
        )
    </insert>

    <!--table10 지정석 상품등록-->
    <insert id="reservedPriceInsert"
            parameterType="com.Gosk.GoskProject20221221.domain.TimePrice"
            useGeneratedKeys="true"
            keyProperty="reserved_price_id"
    >
        insert into
        reserved_price_mst(
        reserved_price_id,
        reserved_time,
        reserved_price
        )
        values(
        0,
        #{reserved_time},
        #{reserved_price}
        )
    </insert>


