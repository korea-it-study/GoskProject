        )
    </insert>

    <!--table9 정액권 기간 상품등록-->
    <insert id="commuterDpPriceInsert"
            parameterType="com.Gosk.GoskProject20221221.domain.TimePrice"
            useGeneratedKeys="true"
            keyProperty="commuter_dp_id"
    >
        insert into
        commuter_day_price_mst(
